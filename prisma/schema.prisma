generator client {
  provider               = "prisma-client"
  output                 = "../generated/prism"
  runtime                = "bun"
  generatedFileExtension = "ts"
  importFileExtension    = "ts"
  moduleFormat           = "esm"
}

datasource db {
  provider = "mysql"
}

model SchemaVersion {
  id Int @id @default(1)
  version Int 
}

